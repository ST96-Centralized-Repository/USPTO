<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:pat="http://www.wipo.int/standards/XMLSchema/ST96/Patent" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.wipo.int/standards/XMLSchema/ST96/Patent" elementFormDefault="qualified" attributeFormDefault="qualified" version="V4_0">
<xsd:element name="FullAssignmentIndicator" type="xsd:boolean">
		<xsd:annotation>
			<xsd:documentation>Full assignment indicator is set to true if the patent is 100% assigned, that is, if there is no partial assignment or the partial assignments with fractional interests total 100%.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
</xsd:schema>
