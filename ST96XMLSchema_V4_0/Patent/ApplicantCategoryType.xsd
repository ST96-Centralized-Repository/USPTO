<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:pat="http://www.wipo.int/standards/XMLSchema/ST96/Patent" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.wipo.int/standards/XMLSchema/ST96/Patent" elementFormDefault="qualified" attributeFormDefault="qualified" version="V4_0">
<xsd:simpleType name="ApplicantCategoryType">
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="Applicant_Inventor">
				<xsd:annotation>
					<xsd:documentation>Indicates whether the applicant is an inventor</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Applicant">
				<xsd:annotation>
					<xsd:documentation>Indicates whether an applicant is applicant only</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
</xsd:schema>
