<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:pat="http://www.wipo.int/standards/XMLSchema/ST96/Patent" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.wipo.int/standards/XMLSchema/ST96/Patent" elementFormDefault="qualified" attributeFormDefault="qualified" version="V4_0">
<xsd:element name="PriorUseRightFromDiscontinuationIndicator" type="xsd:boolean" fixed="true">
		<xsd:annotation>
			<xsd:documentation>Indication that a third party right to use the invention may exist, based on the earlier discontinuation of a patent, or patent application, or the earlier cessation of a patent that was later revived</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
</xsd:schema>
