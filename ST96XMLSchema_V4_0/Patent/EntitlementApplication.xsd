<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:pat="http://www.wipo.int/standards/XMLSchema/ST96/Patent" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.wipo.int/standards/XMLSchema/ST96/Patent" elementFormDefault="qualified" attributeFormDefault="qualified" version="V4_0">
<xsd:include schemaLocation="EntitlementApplicationType.xsd"/>
<xsd:element name="EntitlementApplication" type="pat:EntitlementApplicationType">
		<xsd:annotation>
			<xsd:documentation>The Office can order that a person may make a new patent application, claiming the filing date of a previous application or granted patent, in circumstances where there has been a dispute over the ownership of a patent application or a granted patent.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
</xsd:schema>
