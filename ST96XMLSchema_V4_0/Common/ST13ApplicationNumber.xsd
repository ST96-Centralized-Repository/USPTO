<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:com="http://www.wipo.int/standards/XMLSchema/ST96/Common" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.wipo.int/standards/XMLSchema/ST96/Common" elementFormDefault="qualified" attributeFormDefault="qualified" version="V4_0">
<xsd:include schemaLocation="ST13ApplicationNumberType.xsd"/>
<xsd:element name="ST13ApplicationNumber" type="com:ST13ApplicationNumberType">
		<xsd:annotation>
			<xsd:documentation>Application number format recommended in WIPO Standard ST.13. The sequence of indispensable elements in the application number format is IP type (2 digits), year designation (4 digits) and serial number (9 digits).</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
</xsd:schema>
