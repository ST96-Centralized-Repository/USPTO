<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:dgn="http://www.wipo.int/standards/XMLSchema/ST96/Design" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.wipo.int/standards/XMLSchema/ST96/Design" elementFormDefault="qualified" attributeFormDefault="qualified" version="V4_0">
	<xsd:attribute name="priorRefusalIndicator" type="xsd:boolean">
		<xsd:annotation>
			<xsd:documentation>Indication of whether the decision follows a refusal or not. If true, the decision follows a refusal, if false, the decision does not follow a refusal</xsd:documentation>
		</xsd:annotation>
	</xsd:attribute>
</xsd:schema>
